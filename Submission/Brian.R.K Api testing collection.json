    {
      "name": "Get Pet by ID",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "// Verify we have a pet ID from previous request",
              "const petId = pm.environment.get(\"created_pet_id\");",
              "",
              "if (!petId) {",
              "    console.warn(\"‚ö†Ô∏è No pet ID found in environment. Using a known test ID.\");",
              "    pm.environment.set(\"created_pet_id\", \"1\"); // Fallback to a known existing pet ID",
              "}",
              "",
              "console.log(\"Fetching pet with ID: \" + pm.environment.get(\"created_pet_id\"));"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        },
        {
          "listen": "test",
          "script": {
            "exec": [
              "// Get Pet by ID (valid numeric ID) - Expect 200",
              "pm.test(\"Status is 200\", function() {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "pm.test(\"Pet has ID and name\", function() {",
              "    const pet = pm.response.json();",
              "    pm.expect(pet).to.have.property('id');",
              "    pm.expect(pet).to.have.property('name');",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
