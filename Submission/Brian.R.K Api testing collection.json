    {
      "name": "complete pet lifecycle test",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "// Complete workflow test",
              "console.log(\"üîÑ Starting Complete Pet Lifecycle Test\");",
              "",
              "// Step 1: Create a new pet",
              "const createPetRequest = {",
              "    url: pm.environment.get(\"base_url\") + \"/pet\",",
              "    method: 'POST',",
              "    header: {",
              "        'Content-Type': 'application/json',",
              "        'Accept': 'application/json'",
              "    },",
              "    body: {",
              "        mode: 'raw',",
              "        raw: JSON.stringify({",
              "            id: Math.floor(Math.random() * 10000) + 1000,",
              "            category: { id: 1, name: \"Dogs\" },",
              "            name: \"Workflow_Pet_\" + Date.now(),",
              "            photoUrls: [\"https://example.com/workflow.jpg\"],",
              "            tags: [{ id: 1, name: \"test\" }],",
              "            status: \"available\"",
              "        })",
              "    }",
              "};",
              "",
              "// Execute create request",
              "pm.sendRequest(createPetRequest, function (err, response) {",
              "    if (err) {",
              "        console.error(\"Error creating pet:\", err);",
              "        return;",
              "    }",
              "    ",
              "    console.log(\"‚úÖ Pet created. Status:\", response.code);",
              "    const petData = response.json();",
              "    pm.environment.set(\"workflow_pet_id\", petData.id);",
              "    pm.environment.set(\"workflow_pet_name\", petData.name);",
              "    ",
              "    // Step 2: Get the created pet",
              "    const getPetRequest = {",
              "        url: pm.environment.get(\"base_url\") + \"/pet/\" + petData.id,",
              "        method: 'GET',",
              "        header: { 'Accept': 'application/json' }",
              "    };",
              "    ",
              "    pm.sendRequest(getPetRequest, function (err, response) {",
              "        if (err) {",
              "            console.error(\"Error getting pet:\", err);",
              "            return;",
              "        }",
              "        ",
              "        console.log(\"‚úÖ Pet retrieved. Status:\", response.code);",
              "        ",
              "        // Verify data matches",
              "        const retrievedPet = response.json();",
              "        if (retrievedPet.name === petData.name) {",
              "            console.log(\"‚úÖ Data consistency verified\");",
              "        } else {",
              "            console.error(\"‚ùå Data mismatch!\");",
              "        }",
              "        ",
              "        // Step 3: Test error scenarios",
              "        const invalidRequest = {",
              "            url: pm.environment.get(\"base_url\") + \"/pet/999999999\",",
              "            method: 'GET',",
              "            header: { 'Accept': 'application/json' }",
              "        };",
              "        ",
              "        pm.sendRequest(invalidRequest, function (err, response) {",
              "            console.log(\"‚úÖ Invalid ID test completed. Status:\", response.code);",
              "            ",
              "            // Final summary",
              "            console.log(\"=\".repeat(50));",
              "            console.log(\"üîÑ Workflow test completed successfully!\");",
              "            console.log(\"Created Pet ID:\", petData.id);",
              "            console.log(\"Created Pet Name:\", petData.name);",
              "        });",
              "    });",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
