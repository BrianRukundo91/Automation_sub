    {
      "name": "Add pet",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "// Generate random pet ID to avoid conflicts",
              "const randomId = Math.floor(Math.random() * 10000) + 1000;",
              "pm.environment.set(\"generated_pet_id\", randomId);",
              "",
              "// Generate random pet name",
              "const petNames = [\"Buddy\", \"Max\", \"Charlie\", \"Lucy\", \"Daisy\", \"Molly\", \"Bailey\", \"Rocky\"];",
              "const randomName = petNames[Math.floor(Math.random() * petNames.length)];",
              "pm.environment.set(\"random_pet_name\", randomName);",
              "",
              "// Set current timestamp",
              "pm.environment.set(\"timestamp\", new Date().toISOString());",
              "",
              "// Generate unique pet data",
              "const uniqueData = {",
              "    name: `${randomName}_${randomId}`,",
              "    timestamp: pm.environment.get(\"timestamp\")",
              "};",
              "pm.environment.set(\"unique_pet_name\", uniqueData.name);"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        },
        {
          "listen": "test",
          "script": {
            "exec": [
              "// POST /pet - Create Valid Pet",
              "pm.test(\"Status is 200\", function() {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "const pet = pm.response.json();",
              "",
              "pm.test(\"Response has pet ID\", function() {",
              "    pm.expect(pet).to.have.property('id');",
              "});",
              "",
              "pm.test(\"Response has pet name\", function() {",
              "    pm.expect(pet).to.have.property('name');",
              "    pm.expect(pet.name).to.not.be.empty;",
              "});",
              "",
              "// Save for GET request",
              "pm.environment.set(\"created_pet_id\", pet.id);",
              "pm.environment.set(\"created_pet_name\", pet.name);",
              "",
              "console.log(\"‚úÖ Created pet:\", pet.name, \"(ID:\", pet.id + \")\");"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json",
            "type": "text"
          },
          {
            "key": "Accept",
            "value": "application/json",
            "type": "text"
